                  <a href="#" class="twitter"><i class="bi bi-twitter"></i></a>
                  <a href="#" class="facebook"><i class="bi bi-facebook"></i></a>
                  <a href="#" class="instagram"><i class="bi bi-instagram"></i></a>
                  <a href="#" class="linkedin"><i class="bi bi-linkedin"></i></a>
                </div>
              </div>
            </div>

          </div>

          <div class="col-xl-8">

            <div class="card">
              <div class="card-body pt-3">
                <!-- Bordered Tabs -->
                <ul class="nav nav-tabs nav-tabs-bordered">

                  <li class="nav-item">
                    <button class="nav-link active" data-bs-toggle="tab" data-bs-target="#profile-overview">Overview</button>
                  </li>

                  <li class="nav-item">
                    <button class="nav-link" data-bs-toggle="tab" data-bs-target="#profile-edit">Edit Profile</button>
                  </li>

                  <li class="nav-item">
                    <button class="nav-link" data-bs-toggle="tab" data-bs-target="#profile-change-password">Change Password</button>
                  </li>

                </ul>
                <div class="tab-content pt-2">

                  <div class="tab-pane fade show active profile-overview" id="profile-overview">

                    <div class="row">
                      <div class="col-lg-3 col-md-4 label ">Full Name</div>
                      <div class="col-lg-9 col-md-8"><%= user.firstName %> <%= user.lastName %></div>
                    </div>

                    <div class="row">
                      <div class="col-lg-3 col-md-4 label">Address</div>
                      <div class="col-lg-9 col-md-8"><%= user.country %> | <%= user.town %></div>
                    </div>

                    <div class="row">
                      <div class="col-lg-3 col-md-4 label">Phone</div>
                      <div class="col-lg-9 col-md-8"><%= user.contact %></div>
                    </div>

                    <div class="row">
                      <div class="col-lg-3 col-md-4 label">Email</div>
                      <div class="col-lg-9 col-md-8"><%= user.email %></div>
                    </div>

                  </div>

                  <div class="tab-pane fade profile-edit pt-3" id="profile-edit">
                    <div id="update-profile-alert" class=""></div>
                    <!-- Profile Edit Form -->
                    <form class="form" id="user-profile-edit" autocomplete="off">
                      <div class="col-9 m-auto norrechel-alert bg-success text-white text-center p-2"> 
                        User information updated successfully
                        
                      </div>
                      <fieldset class="profile-edit-fieldset px-3 mt-2">
                        <!-- <legend class="mb-3">Personal Information</legend> -->
                        <div class="row mb-1">
                          <label for="fName" class="col-6 col-form-label">First Name</label>
                          <label for="lName" class="col-6 col-form-label">Last Name</label>
                        </div>
    
                        <div class="row mb-1">                      
                          <div class="col-6">
                            <input name="fName" type="text" class="form-control form-control-sm shadow-none" id="fName" value="<%= user.firstName %>">
                            <div class="invalid-feedback"></div>
                          </div>
                          <div class="col-6">
                            <input name="lName" type="text" class="form-control form-control-sm shadow-none" id="lName" value="<%= user.lastName %>">
                            <div class="invalid-feedback"></div>
                          </div>
                        </div>
    
                        <div class="row mb-1">
                          <label for="phone" class="col-6 col-form-label">Phone</label>
                          <label for="email" class="col-6 col-form-label">Email</label>
                        </div> 
                        <div class="row mb-1">
                          <div class="col-6">
                            <input name="phone" type="text" class="form-control form-control-sm shadow-none" id="phone" value="<%= user.contact %>">
                            <div class="invalid-feedback"></div>
                          </div>
                          <div class="col-6">
                            <input name="email" type="email" class="form-control form-control-sm shadow-none" id="edit-email" value="<%= user.email %>">
                            <div class="invalid-feedback"></div>
                          </div>
                        </div>
                        <div class="row mb-1">
                          <label for="country" class="col-6 col-form-label">Country</label>
                          <label for="town" class="col-6 col-form-label">Town</label>
                        </div>
                        <div class="row mb-1">
                          <div class="col-6">
                            <input name="country" type="text" class="form-control form-control-sm shadow-none" id="country" value="<%= user.country %>">
                            <div class="invalid-feedback"></div>
                          </div>
                          <div class="col-6">
                            <input name="town" type="text" class="form-control form-control-sm shadow-none" id="town" value="<%= user.town %>">
                            <div class="invalid-feedback"></div>
                          </div>
                        </div>
                      </fieldset>
                      <div class="text-center">
                        <button type="submit" name="submit" class="btn btn-sm save-btn">Save Changes</button>
                        <button class="btn btn-sm save-btn d-none loading-btn" type="button" disabled>
                          <span class="spinner-border spinner-border-sm" role="status" aria-hidden="true"></span>
                          <span class="">Loading...</span>
                        </button>
                      </div>
                    </form><!-- End Profile Edit Form -->
                    
                  </div>
                  <div class="tab-pane fade pt-3" id="profile-change-password">
                    <div class="container justify-content-center">
                      <div class="row m-auto justify-content-center align-items-center">
                        <div class="col-10 m-auto justify-content-center">
                          <div class="col-9 m-auto norrechel-alert bg-success text-white text-center p-2"> 
                            User information updated successfully
                            
                          </div>
                          <!-- Change Password Form -->
                          <form class="form" id="password-update" autocomplete="off">
      
                            <div class="row mb-3">
                              <label for="currentPassword" class="col-md-4 col-lg-3 col-form-label">Current Password</label>
                              <div class="col-md-8 col-lg-9">
                                <input name="password" type="password" class="form-control shadown-none" id="currentPassword">
                                <div class="invalid-feedback">Please enter a valid Email Address!</div>
                              </div>
                            </div>
      
                            <div class="row mb-3">
                              <label for="newPassword" class="col-md-4 col-lg-3 col-form-label">New Password</label>
                              <div class="col-md-8 col-lg-9">
                                <input name="newpassword" type="password" class="form-control shadown-none" id="newPassword">
                                <div class="invalid-feedback">Please enter a valid Email Address!</div>
                              </div>
                            </div>
      
                            <div class="row mb-3">
                              <label for="renewPassword" class="col-md-4 col-lg-3 col-form-label">Re-enter New Password</label>
                              <div class="col-md-8 col-lg-9">
                                <input name="confirm" type="password" class="form-control" id="renewPassword">
                                <div class="invalid-feedback">Please enter a valid Email Address!</div>
                              </div>
                            </div>
      
                            <div class="text-center">
                              <button type="submit" name="submit" class="btn btn-primary">Change Password</button>
                              <button class="btn btn-sm d-none loading-btn" type="button" disabled>
                                <span class="spinner-border spinner-border-sm" role="status" aria-hidden="true"></span>
                                <span class="">Loading...</span>
                              </button>
                            </div>
                          </form><!-- End Change Password Form -->
                        </div>
                      </div>
                    </div>

                  </div>

                </div><!-- End Bordered Tabs -->

              </div>
            </div>

          </div>
        </div>
      </section>
      </div>
    </div> 
    <div class="offcanvas offcanvas-end" tabindex="-1" id="item-update-contents" aria-labelledby="offcanvasRightLabel">
      <div class="offcanvas-header">
        <h5 id="offcanvasRightLabel">Update Item Information</h5>
        <button type="button" class="btn-close text-reset" data-bs-dismiss="offcanvas" aria-label="Close"></button>
      </div>
      <div class="offcanvas-body">
        <div class="container">
          <div class="row">
            <form class="needs-validation" novalidate id="admin-update-item-form" autocomplete="off">
              <div class="row mb-1 justify-content-center">
                <div class="col-10 mb-2 norrechel-alert bg-success text-white text-center p-2 "> 
                  Item added successfully
                  
                </div>
              </div>
              <div class="row mb-1 justify-content-center update-row">
                <label for="item_update_photo" class="col-md-4 col-lg-4 col-form-label">Item Image</label>
                <div class="col-8">                       
                  <div class="pt-2 d-flex">
                    <label for="item_update_photo" class="btn text-light upload_icon" title="Upload an item photo"><i class="class bi bi-upload"></i></label>
                    <input type="file" name="file" id="item_update_photo" hidden required>
                    <img src="" alt="item photo" class="item_updatePreview">
                    <div class="invalid-feedback">Please enter a valid Email Address!</div>
                  </div>
                </div>
              </div>

              <div class="row mb-1 justify-content-center update-row">
                <label for="item_updateName" class="col-6 col-form-label">Name</label>  
                <label for="item_category_update" class="col-6 col-form-label">Category</label>
              </div>
              <div class="row mb-1 justify-content-center update-row">
                <div class="col-6">
                  <input type="hidden" name="_id" id="item_id">
                  <input name="name" type="text" class="form-control form-control-sm shadow-none" id="item_updateName" required>
                  <div class="invalid-feedback">Please enter a valid Email Address!</div>
                </div> 
                <div class="col-6">
                  <select name="category" id="item_category_update" class="form-select form-select-sm shadow-none" aria-label="Default select example">
                      <option value=""></option>
                      <% if (categories.length == 1) { %>
                        <option value="<%= categories.name %>"><%= categories.name %></option>
                      <% } %>
                      <% for( let i in categories ) { %>                              
                        <option value="<%= categories[i].name %>"><%= categories[i].name %></option>
                      <% } %>
                  </select>  
                  <div class="invalid-feedback">Please enter a valid Email Address!</div>
                </div>
              </div>
              <div class="row mb-1 justify-content-center update-row">
                <label for="update_quantity" class="col-12 col-form-label">Quantity</label>                    
                <div class="col-12">
                  <input type="number" name="quantity" class="form-control form-control-sm shadow-none" id="update_quantity" placeholder="e.g. 50" required>
                  <div class="invalid-feedback">Please enter a valid Email Address!</div>
                </div>
              </div>  
              
              <div class="row mb-1 justify-content-center update-row">
                <label for="update_price" class="col-6 col-form-label">Price</label>  
                <label for="update_discount" class="col-6 col-form-label">discount</label>
              </div>
              <div class="row mb-1 justify-content-center update-row">
                <div class="col-6">
                  <input name="price" type="number" class="form-control form-control-sm shadow-none" id="update_price" required>
                  <div class="invalid-feedback">Please enter a valid Email Address!</div>
                </div> 
                <div class="col-6">
                  <input name="discount" type="number" class="form-control form-control-sm shadow-none" id="update_discount" required>
                  <div class="invalid-feedback">Please enter a valid Email Address!</div>
                </div>
              </div>


              <div class="row mb-1 justify-content-center update-row">
                <label for="update_supplier" class="col-12 col-form-label">Supplier</label>                    
                <div class="col-12">
                  <input type="text" name="supplier" class="form-control form-control-sm shadow-none" id="update_supplier" placeholder="" required>
                  <div class="invalid-feedback">Please enter a valid Email Address!</div>
                </div>
              </div>
              
              <div class="row mb-1 justify-content-center update-row">
                <label for="update_about" class="col-12 col-form-label">Description</label>
                <div class="col-12">
                  <textarea name="about" class="form-control form-control-sm shadow-none" spellcheck="true" id="update_about" style="height: 100px"></textarea>
                  <div class="invalid-feedback">Please enter a valid Email Address!</div>
                </div>
              </div>

              <div class="row mb-1 justify-content-center update-row">
                <label for="update_details" class="col-12 col-form-label">Details</label>
                <div class="col-12">
                  <textarea name="details" class="form-control form-control-sm shadow-none" spellcheck="true" id="update_details" style="height: 100px"></textarea>
                  <div class="invalid-feedback">Please enter a valid Email Address!</div>
                </div>
              </div>

              <div class="text-center update-row">
                <button type="submit" name="submit" class="btn btn-sm w-75">Update</button>
                <button class="btn btn-sm w-75 d-none loading-btn" type="button" disabled>
                  <span class="spinner-border spinner-border-sm" role="status" aria-hidden="true"></span>
                  <span class="">Loading...</span>
              </button>
              </div>
            </form><!-- End add item Form -->
          </div>
        </div>
      </div>
    </div>
    <div class="offcanvas offcanvas-end" tabindex="-1" id="category-update-content" aria-labelledby="categoryLabel">
      <div class="offcanvas-header">
        <h5 id="categoryLabel">Category Update</h5>
        <button type="button" class="btn-close text-reset" data-bs-dismiss="offcanvas" aria-label="Close"></button>
      </div>
      <div class="offcanvas-body">
        <form class="needs-validation p-0" novalidate id="norrechel-category-update-form" autocomplete="off">
          <div class="row mb-3 justify-content-center">
            <div class="col-10 mb-2 norrechel-alert bg-success text-white text-center p-2 "> 
              Category added successfully
              
            </div>
            <div class="col-10 mb-2 update-row">
              <label for="ref-name" class="form-label">Name</label>
              <input type="text" name="ref_name" placeholder="category name" class="form-control form-control-sm shadow-none" id="ref-name">
              <div class="invalid-feedback">Please enter a valid Email Address!</div>
            </div>
            <div class="col-10 mb-2 update-row">
              <label for="category-update-name" class="form-label">Name</label>
              <input type="text" name="name" placeholder="New name" class="form-control form-control-sm shadow-none" id="category-update-name">
              <div class="invalid-feedback">Please enter a valid Email Address!</div>
            </div>
            <div class="text-center update-row">
                <button type="submit" name="submit" class="btn btn-sm w-75">Update</button>
                <button class="btn btn-sm w-75 d-none loading-btn" type="button" disabled>
                  <span class="spinner-border spinner-border-sm" role="status" aria-hidden="true"></span>
                  <span class="">Loading...</span>
              </button>
            </div>
          </div>
        </form><!-- End add item Form -->
      </div>
    </div>
  </main><!-- End #main -->