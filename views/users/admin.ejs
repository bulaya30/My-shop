                        <label for="discount" class="col-4 col-form-label">discount</label>
                      </div>
                      <div class="row mb-1 justify-content-center">
                        <div class="col-4">
                          <input name="price" type="number" class="form-control form-control-sm shadow-none" id="price" required>
                          <div class="invalid-feedback">Please enter a valid Email Address!</div>
                        </div> 
                        <div class="col-4">
                          <input name="discount" type="number" class="form-control form-control-sm shadow-none" id="discount" required>
